{
  "manifest_version": 2,
  "name": "youmix",
  "description": "Inject extra controls into youtube videos for audio mixing",
  "content_scripts": [{
      "js": [
        "lib/jquery.min.js",
        "lib/rangeslider.min.js",
        "src/youmix.js"
      ],
      "css": [
        "lib/rangeslider.css"
      ],
      "matches":  [
        "*://www.youtube.com/*"
      ]
  }],
  "version": "0.0.1",
  "web_accessible_resources": [
    "templates/*"
  ]
}