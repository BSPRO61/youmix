{
  "manifest_version": 2,
  "name": "youmix",
  "description": "Chrome extension for adding audio effects to YouTube videos",
  "content_scripts": [{
      "js": [
        "lib/jquery.min.js",
        "lib/rangeslider.min.js",
        "src/youmix.js"
      ],
      "css": [
        "lib/rangeslider.css",
        "src/youmix.css"
      ],
      "matches":  [
        "*://www.youtube.com/*"
      ]
  }],
  "version": "0.0.1",
  "web_accessible_resources": [
    "src/youmix.html"
  ],
  "icons": { 
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}